    "dayjs": "^1.11.10",
    "expo": "^50.0.17",
    "expo-application": "~5.8.4",
    "expo-auth-session": "~5.4.0",
    "expo-av": "~13.10.6",
    "expo-blur": "~12.9.2",
    "expo-clipboard": "~5.0.1",
    "expo-config-plugin-ios-share-extension": "^0.0.4",
    "expo-crypto": "~12.8.1",
    "expo-document-picker": "~11.10.1",
