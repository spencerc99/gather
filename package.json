{
  "name": "gather",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "scripts": {
    "start": "expo start --dev-client",
    "clean": "expo prebuild --clean",
    "prebuild": "expo prebuild --no-install",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "test": "jest --watchAll",
    "eslint-fix": "eslint --fix .",
    "tsc": "tsc",
    "postinstall": "patch-package",
    "android": "expo run:android",
    "build-ios": "EAS_LOCAL_BUILD_ARTIFACTS_DIR=\"./builds\" eas build -p ios --local --non-interactive",
    "submit-ios": "eas submit -p ios --non-interactive --path=\"./builds/$(ls -1 builds | sort | tail -n 1)\"",
    "build-and-submit-ios": "npm run build-ios && npm run submit-ios"
  },
  "jest": {
    "preset": "jest-expo"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.0.0",
    "@react-native-async-storage/async-storage": "1.21.0",
